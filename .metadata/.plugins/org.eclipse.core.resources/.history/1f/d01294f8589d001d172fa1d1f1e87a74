package algo;

import java.util.Scanner;

public class a9 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String str =sc.next();
		String str2 = "";
		char[] c = str.toCharArray();
		for(int i =0; i<c.length;i++) {
			if(80<=c[i]&&c[i]<=89) {
				str2+=c[i];
			}
		}
		System.out.println(str2);
	}

}
