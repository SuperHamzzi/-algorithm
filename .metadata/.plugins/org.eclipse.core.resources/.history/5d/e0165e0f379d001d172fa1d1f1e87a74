package algo;

import java.util.Scanner;

public class a5 {
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
		String b = sc.next();
		char[] s = b.toCharArray();
		int lt= 0;
		int rt= s.length-1;
		while(lt>rt){
			if(65<=s[lt]&&s[lt]<=95&&65<=s[rt]&&s[rt]<=95) {
				char temp = s[lt];
				s[lt]=s[rt];
				s[rt]=temp;
			}
			if(97<=s[lt]&&s[lt]<=122&&97<=s[rt]&&s[rt]<=122) {
				char temp = s[lt];
				s[lt]=s[rt];
				s[rt]=temp;
			}
		}
		
	
	}
}
